{
  "expo": {
    "name": "RSXP",
    "slug": "rsxp",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#100F12"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.rocketseat.rsxp"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rocketseat.rsxp"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "json"]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#100F12"
    }
  }
}
